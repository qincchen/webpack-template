{
  "extends": "google",
  "plugins": [
    "react"
  ],
  "env": {
//    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "__DEV__": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    "max-len": 0,
    "padded-blocks": 0,
    "arrow-parens": 0,
    "no-unused-vars": 1,
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "require-jsdoc": 0,
    "no-undef": 2,
    "semi": [
      1,
      "always"
    ],
    "react/jsx-quotes": 0,
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1,
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "strict": 1
  }
}
